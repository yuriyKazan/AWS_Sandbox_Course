    "policy_content": {
      "Statement": [
        {
          "Action": [
            "logs:*",
            "dynamodb:*",
            "ssm:*",
            "kms:*",
            "cognito-idp:*"
          ],
          "Effect": "Allow",
          "Resource": "*"
        }
      ],
      "Version": "2012-10-17"
    },
    "resource_type": "iam_policy"
  },
